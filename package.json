{
  "name": "@zpcscc/generator",
  "version": "0.1.0",
  "description": "低代码表单生成器",
  "homepage": "https://dxsixpc.github.io/generator",
  "repository": "https://github.com/dxsixpc/generator",
  "license": "MIT",
  "author": {
    "name": "dxsix",
    "email": "dxsixpc@gmail.com"
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "commit": "git add . && git-cz",
    "deploy": "pnpm run docs:build && pnpm run docs:deploy",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "doctor": "father doctor",
    "lint": "eslint --cache --ext .js,.jsx,.mjs,.ts,.tsx src",
    "prepare": "husky install && dumi setup",
    "prepublishOnly": "father doctor && pnpm run build",
    "start": "pnpm run dev"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@ant-design/icons": "5.3.7",
    "@dnd-kit/core": "6.1.0",
    "@dnd-kit/modifiers": "7.0.0",
    "@dnd-kit/sortable": "8.0.0",
    "@dnd-kit/utilities": "3.2.2",
    "@emotion/css": "11.11.2",
    "@emotion/react": "11.11.4",
    "@emotion/styled": "11.11.5",
    "@emotion/utils": "1.2.1",
    "@zpcscc/components": "^1.0.1",
    "@zpcscc/utils": "7.0.0",
    "antd": "^5.18.0",
    "lodash": "4.17.21",
    "react-copy-to-clipboard": "5.1.0",
    "recoil": "0.7.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@emotion/babel-plugin": "11.10.8",
    "@province-city-china/area": "8.5.7",
    "@province-city-china/city": "8.5.7",
    "@province-city-china/province": "8.5.7",
    "@types/lodash": "4.17.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "18.3.0",
    "@zpcscc/configs": "^2.0.6",
    "classnames": "2.5.1",
    "dumi": "^2.3.8",
    "eslint": "8.52.0",
    "father": "^4.4.4",
    "gh-pages": "6.1.1",
    "husky": "9.0.11",
    "lint-staged": "^15.2.5",
    "postcss": "8.4.38",
    "prettier": "^3.3.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-is": "18.3.1",
    "styled-components": "6.1.11",
    "stylelint": "14.16.1",
    "typescript": "5.4.5",
    "webpack": "5.91.0"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
